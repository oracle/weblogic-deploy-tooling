    "wlst_type": "EmbeddedLDAP",
    "online_bean": "weblogic.management.configuration.EmbeddedLDAPMBean",
    "default_name_value": "%DOMAIN%",
    "short_name": "LDAP",
    "folders": {},
    "attributes": {
        "AnonymousBindAllowed":    [ {"version": "[10,)", "wlst_mode": "both", "wlst_name": "AnonymousBindAllowed",    "wlst_path": "WP001", "default_value": "false",  "wlst_type": "boolean"   } ],
        "BackupCopies":            [ {"version": "[10,)", "wlst_mode": "both", "wlst_name": "BackupCopies",            "wlst_path": "WP001", "default_value": 7,        "wlst_type": "integer"   } ],
        "BackupHour":              [ {"version": "[10,)", "wlst_mode": "both", "wlst_name": "BackupHour",              "wlst_path": "WP001", "default_value": 23,       "wlst_type": "integer"   } ],
        "BackupMinute":            [ {"version": "[10,)", "wlst_mode": "both", "wlst_name": "BackupMinute",            "wlst_path": "WP001", "default_value": 5,        "wlst_type": "integer"   } ],
        "CacheEnabled":            [ {"version": "[10,)", "wlst_mode": "both", "wlst_name": "CacheEnabled",            "wlst_path": "WP001", "default_value": "true",   "wlst_type": "boolean"   } ],
        "CacheSize":               [ {"version": "[10,)", "wlst_mode": "both", "wlst_name": "CacheSize",               "wlst_path": "WP001", "default_value": 32,       "wlst_type": "integer"   } ],
        "CacheTTL":                [ {"version": "[10,)", "wlst_mode": "both", "wlst_name": "CacheTTL",                "wlst_path": "WP001", "default_value": 60,       "wlst_type": "integer"   } ],
        "CredentialEncrypted":     [ {"version": "[10,)", "wlst_mode": "both", "wlst_name": "CredentialEncrypted",     "wlst_path": "WP001", "default_value": null, "derived_default": "${:true}",  "wlst_type": "password", "get_method": "GET" } ],
        "KeepAliveEnabled":        [ {"version": "[10,)", "wlst_mode": "both", "wlst_name": "KeepAliveEnabled",        "wlst_path": "WP001", "default_value": "false",  "wlst_type": "boolean"   } ],
        "MasterFirst":             [ {"version": "[10,)", "wlst_mode": "both", "wlst_name": "MasterFirst",             "wlst_path": "WP001", "default_value": "false",  "wlst_type": "boolean"   } ],
        "Notes":                   [ {"version": "[10,)", "wlst_mode": "both", "wlst_name": "Notes",                   "wlst_path": "WP001", "default_value": null,     "wlst_type": "string"    } ],
        "RefreshReplicaAtStartup": [ {"version": "[10,)", "wlst_mode": "both", "wlst_name": "RefreshReplicaAtStartup", "wlst_path": "WP001", "default_value": "false",  "wlst_type": "boolean"   } ],
        "Timeout":                 [ {"version": "[10,)", "wlst_mode": "both", "wlst_name": "Timeout",                 "wlst_path": "WP001", "default_value": 0,        "wlst_type": "integer"   } ]
    },
    "wlst_attributes_path": "WP001",
    "wlst_paths": {
        "WP001": "/EmbeddedLDAP/%EMBEDDEDLDAP%"
    }
}
